{
  "version": 1,
  "default_encoding": "utf-8",
  "allowed_encodings": [
    "utf-8",
    "utf-8-sig"
  ],
  "include_globs": [
    "*.py",
    "**/*.py",
    "*.md",
    "**/*.md",
    "*.toml",
    "**/*.toml",
    "*.json",
    "**/*.json",
    "*.yaml",
    "**/*.yaml",
    "*.yml",
    "**/*.yml",
    "*.txt",
    "**/*.txt",
    "*.csv",
    "**/*.csv"
  ],
  "exclude_globs": [
    ".git/**",
    "**/.git/**",
    ".venv/**",
    "**/.venv/**",
    "venv/**",
    "**/venv/**",
    "node_modules/**",
    "**/node_modules/**",
    "__pycache__/**",
    "**/__pycache__/**",
    "dist/**",
    "**/dist/**",
    "build/**",
    "**/build/**"
  ],
  "high_risk_tokens": [
    "�",
    "锟斤拷"
  ],
  "warn_tokens": [
    "鍥",
    "鏂",
    "璇",
    "锛",
    "鈥",
    "銆",
    "馃"
  ],
  "enforcement": {
    "mode": "block",
    "warn_threshold": 1,
    "block_threshold": 1
  }
}
